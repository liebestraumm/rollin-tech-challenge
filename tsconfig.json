{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true, 
    "removeComments": true, 
    "incremental": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "sequelize"]
}
